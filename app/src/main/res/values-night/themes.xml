<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme.SKY1" parent="Theme.Material3.DayNight">
        <!-- Primary brand color. -->
        <!--        android:windowLightStatusBar：控制浅色模式下状态栏文字颜色。-->
        <!--        colorPrimary、colorSecondary：定义主色与辅助色。-->
        <!--标题背景色，toolbar（actionbar）颜色 -->
        <!--        <item name="colorPrimary">@color/colorPrimary</item>-->-->
        <!-- Primary brand color. -->
        <!--标题背景色，toolbar（actionbar）颜色 -->
        <item name="colorPrimary">@color/primary</item>
        <!--状态栏背景色，以前是colorPrimaryDark-->
        <item name="colorPrimaryVariant">@color/teal_200</item>
        <!--        buton的颜色？我去-->
        <item name="colorOnPrimary">@color/green</item>
        <!-- Secondary brand color.可以控制float按钮 -->
        <item name="colorSecondary">@color/teal_200</item>
<!--        <item name="colorSecondaryVariant">@color/teal_700</item>-->
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <!-- Customize your theme here. -->
        <item name="android:textColor">@color/color_333333</item>
    </style>
</resources>